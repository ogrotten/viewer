{
  "hosting": {
    "public": "./.firebase/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "./.firebase/public/index.html"
      }
    ]
  },
  "rewrites": [
    {
      "source": "**",
      "destination": "/app.html"
    }
  ]
}